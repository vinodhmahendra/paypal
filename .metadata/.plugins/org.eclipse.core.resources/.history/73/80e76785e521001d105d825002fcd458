package paypal.demos;

public class ApplicationGenericsClass {

	public static void main(String[] args) {
		SampleClass<Integer> intObj = new SampleClass<>(10);
		

	}

}

//generic class
class SampleClass <T> {
	
	//variable of T type
	private T data;
	
	public SampleClass(T data) {
		this.data = data;
	}
	
	public T getData() {
		return data;
	}
	
}